{
    "name": "symfony/framework-standard-edition",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.1.*",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "1.1.*",
        "twig/extensions": "1.0.*@dev",
        "symfony/assetic-bundle": "2.1.*",
        "symfony/swiftmailer-bundle": "2.1.*",
        "symfony/monolog-bundle": "2.1.*",
        "sensio/distribution-bundle": "2.1.*",
        "sensio/framework-extra-bundle": "2.1.*",
        "sensio/generator-bundle": "2.1.*",
        "jms/security-extra-bundle": "1.2.*",
        "jms/di-extra-bundle": "1.1.*",
        "kriswallsmith/assetic": "1.1.*@dev",
        "friendsofsymfony/user-bundle": "2.0.*@dev",
        "friendsofsymfony/jsrouting-bundle": "1.1.*",
        "knplabs/knp-menu-bundle": ">=1.1.0,<2.0.x-dev",
        "sonata-project/jquery-bundle": "2.1.*",
        "sonata-project/exporter": "2.1.*",
        "sonata-project/block-bundle": "2.1.*",
        "sonata-project/cache-bundle": "2.1.*",
        "sonata-project/admin-bundle": "2.1.*",
        "sonata-project/intl-bundle": "2.1.*",
        "sonata-project/markitup-bundle": "2.1.*",
        "sonata-project/notification-bundle": "2.1.*",
        "egeloen/ckeditor-bundle": "2.*",
        "sonata-project/news-bundle": "2.1.*",
        "sonata-project/user-bundle": "2.1.*",
        "sonata-project/media-bundle": "2.1.*",
        "sonata-project/formatter-bundle": "2.1.*",
        "sonata-project/easy-extends-bundle": "2.1.*",
        "sonata-project/google-authenticator": "2.1.*",
        "sonata-project/doctrine-extensions": "2.1.*",
        "knplabs/gaufrette": "dev-master",
        "simplethings/entity-audit-bundle": "dev-master",
        "knplabs/knp-markdown-bundle": "1.1.*",        
        "sonata-project/exporter": "1.1.*",
        "sonata-project/doctrine-orm-admin-bundle": "dev-master"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web"
    }
}
